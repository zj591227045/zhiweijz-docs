{
  "title": "只为记账 - 用户文档",
  "description": "智能家庭记账应用的完整用户文档",
  "author": "只为记账团队",
  "language": "zh-hans",
  "gitbook": "3.2.3",
  "root": ".",
  "structure": {
    "readme": "README.md",
    "summary": "SUMMARY.md"
  },
  "plugins": [
    "theme-default",
    "search",
    "lunr",
    "sharing",
    "fontsettings",
    "highlight",
    "github",
    "edit-link",
    "prism",
    "splitter"
  ],
  "pluginsConfig": {
    "theme-default": {
      "showLevel": true
    },
    "sharing": {
      "facebook": false,
      "twitter": false,
      "google": false,
      "weibo": true,
      "instapaper": false,
      "vk": false,
      "all": [
        "facebook", "google", "twitter", "weibo", "instapaper"
      ]
    },
    "fontsettings": {
      "theme": "white",
      "family": "sans",
      "size": 2
    },
    "github": {
      "url": "https://github.com/zj591227045/zhiweijz"
    },
    "edit-link": {
      "base": "https://github.com/zj591227045/zhiweijz-docs/edit/main",
      "label": "编辑此页"
    },
    "prism": {
      "css": [
        "prismjs/themes/prism-tomorrow.css"
      ]
    }
  },
  "variables": {
    "version": "1.8.x",
    "website": "https://your-domain.com",
    "support_email": "support@your-domain.com"
  }
}
